 pipeline{
        agent any

        tools {
                maven "Maven 3.8.4"
        }

        stages{
            stage('Initialize'){
               steps{
                   echo "PATH = ${M2_HOME}/bin:${PATH}"
                   echo "M2_HOME = /opt/maven"
               }
            }

           stage('Clean'){
            steps{
            mvn clean
            }
           }

        }
 }